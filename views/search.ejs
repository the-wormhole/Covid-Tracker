<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/search.css">
    <title>Search results</title>
</head>
<body>
    <div id="heading">
        <h1>Search Results for &nbsp;</h1><h3 style="display: inline-block;"><i><%= s %></i></h3>
    </div>
    <section>
        <% for(let res of results){ %>
            <div class="widget">
                <h2 class="res-head"><a href= <%= res.url  %>> <%= res.title %> </a></h2>
                <a href=<%= res.url  %> class="res-url"><%= res.url  %></a>
            </div> 
        <% } %>
        <!-- <div class="widget">
            <h2 class="res-head"><a href= "www.google.com"> Title </a></h2>
            <a href="www.google.com" class="res-url">www.google.com</a>
        </div> -->
    </section>
</body>
</html>